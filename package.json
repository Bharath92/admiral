    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "ejs": "^2.5.6",
    "express": "4.14.0",
    "fs-extra": "^0.30.0",
    "glob": "7.0.5",
    "grunt": "1.0.1",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-uglify": "1.0.1",
    "grunt-contrib-watch": "1.0.0",
    "js-yaml": "3.6.1",
    "method-override": "2.3.6",
    "mimelib": "0.2.19",
    "moment": "2.14.1",
    "mongoose": "4.5.5",
    "node-uuid": "1.4.7",
    "oauth": "0.9.14",
    "pg": "6.0.2",
    "pg-hstore": "2.3.2",
    "proxy-agent": "2.0.0",
    "redis": "2.6.2",
    "remove-markdown": "0.1.0",
    "request": "2.73.0",
    "serve-favicon": "^2.4.1",
